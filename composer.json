{
  "name": "ludo237/slugify",
  "description": "Package that generates slugify words like Gfycat or Twitch Clips",
  "license": "MIT",
  "authors": [
    {
      "name": "Claudio Ludovico",
      "email": "ludo237@outlook.com"
    }
  ],
  "homepage": "https://github.com/ludo237/slugify",
  "keywords": [
    "Laravel",
    "Slugify"
  ],
  "require": {
    "php": "^7.2",
    "illuminate/support": "5.6.x|5.7.x"
  },
  "require-dev": {
    "php": "^7.2",
    "laravel/framework": "5.6.x|5.7.x",
    "phpunit/phpunit": "^7.3",
    "orchestra/testbench": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "Ludo237\\Slugify\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Ludo237\\Slugify\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ludo237\\Slugify\\SlugifyServiceProvider"
      ],
      "aliases": {
        "Slugify": "Ludo237\\Slugify\\Facades\\Slugify"
      }
    }
  }
}
